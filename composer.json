{
    "name": "po/smart-downloader",
    "description": "PHP File downloader in context of test assignment",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^11.4",
        "psalm/phar": "^5.26"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "SmartDownloader\\": "src/SmartDownloader/"
        }
    },
    "version": "1.0.0",
    
    "authors": [
        {
            "name": "pavelo8501",
            "email": "pavelo8501@gmail.com"
        }
    ],
     "scripts": {
        "launch_src": [
            "php src/index.php"
        ],
        "docker_run" : [
            "docker-compose up --build -d"
        ],
        "docker_stoop": [
            "docker-compose down"
        ]
    },
    "require": {
        "ext-pdo_pgsql": "*",
        "catfan/medoo": "^2.1"
    }
}
